{
  "expo": {
    "name": "GillyNative",
    "slug": "GillyNative",
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./assets/gilly-icon.png",

    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.getgilly.gillyapp",
      "buildNumber": "0.0.2"
    },
    "android": {
      "package": "com.getgilly.gillyapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["CAMERA", "CAMERA_ROLL"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "./rn-cli.config.js"
    }
    
  }
}
