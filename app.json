{
  "expo": {
    "name": "GillyNative",
    "slug": "GillyNative",
    "version": "0.0.13",
    "orientation": "portrait",
    "icon": "./assets/gilly-icon-white.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.getgilly",
      "buildNumber": "0.0.13"
    },
    "android": {
      "package": "com.getgilly.gillyapp",
      "versionCode": 13,
      "adaptiveIcon": {
        "foregroundImage": "./assets/gilly-icon.png",

        
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["CAMERA", "CAMERA_ROLL"],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "./rn-cli.config.js"
    },
    "extra":{
      "env": "prod"
    }
  }
}
